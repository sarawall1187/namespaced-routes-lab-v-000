group :development do
  gem 'web-console', '~> 2.0'
end

# In Gemfile:
# Remove: `, '5.0.7.1'` from `gem 'rails', '5.0.7.1'`
# Add: `gem 'listen'`

# Then delete Gemfile.lock
# Run `bundle`

# Run `rake db:seed`
# If successful check the database by using `Song.first` in a Rails Console session